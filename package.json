{
  "name": "phosphor-keymap",
  "version": "0.3.0",
  "description": "A module for keyboard shortcut mapping.",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "dependencies": {
    "clear-cut": "^2.0.1",
    "phosphor-command": "^0.5.0",
    "phosphor-disposable": "^1.0.4"
  },
  "devDependencies": {
    "coveralls": "^2.11.4",
    "css-loader": "^0.23.1",
    "expect.js": "^0.3.1",
    "fs-extra": "^0.26.3",
    "istanbul-instrumenter-loader": "^0.1.3",
    "karma": "^0.13.19",
    "karma-chrome-launcher": "^0.2.0",
    "karma-coverage": "^0.5.0",
    "karma-firefox-launcher": "^0.1.6",
    "karma-ie-launcher": "^0.2.0",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.1.1",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.2.5",
    "rimraf": "^2.4.2",
    "style-loader": "^0.13.0",
    "ts-loader": "^0.7.2",
    "typedoc": "^0.3.11",
    "typescript": "^1.6.2",
    "webpack": "^1.12.9"
  },
  "scripts": {
    "clean": "rimraf lib && rimraf test/build && rimraf test/coverage",
    "clean:example": "rimraf example/bundle.js",
    "build:example": "webpack --config example/webpack.conf.js",
    "build": "tsc --project src",
    "docs": "rimraf docs/api && typedoc --options scripts/tdoptions.json",
    "postinstall": "npm dedupe",
    "prepublish": "npm run build",
    "test:chrome": "karma start --browsers=Chrome test/karma.conf.js",
    "test:coverage": "karma start test/karma-cov.conf.js",
    "test:firefox": "karma start --browsers=Firefox test/karma.conf.js",
    "test:ie": "karma start --browsers=IE test/karma.conf.js",
    "test:debug": "karma start --browsers=Chrome --singleRun=false --debug=true test/karma.conf.js",
    "test": "npm run test:firefox"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/phosphorjs/phosphor-keymap.git"
  },
  "files": [
    "lib/index.js",
    "lib/index.d.ts",
    "lib/keyboard.js",
    "lib/keyboard.d.ts",
    "lib/manager.js",
    "lib/manager.d.ts"
  ],
  "keywords": [
    "keymap",
    "keyboard",
    "keystroke",
    "key",
    "map",
    "shortcut"
  ],
  "author": "S. Chris Colbert <sccolbert@gmail.com>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/phosphorjs/phosphor-keymap/issues"
  },
  "homepage": "https://github.com/phosphorjs/phosphor-keymap"
}
